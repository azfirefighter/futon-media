{
	"name": "futon-media",
	"version": "1.0.0",
	"description": "Instantly stream movies and TV shows for Emby Media",
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"_moduleAliases": {
		"@": "dist"
	},
	"append": true,
	"killSignal": "SIGTERM",
	"killTree": true,
	"script": "dist/index.js",
	"uid": "futon-media",
	"nodemonConfig": {
		"delay": "1000",
		"exec": "NODE_ENV=development node --inspect dist/index.js",
		"ext": "js",
		"restartable": false,
		"watch": "dist"
	},
	"scripts": {
		"deploy": "run-s repo build restart",
		"repo": "git pull; echo; git reset --hard",
		"build": "tsc || true",
		"assert:build": "test ! -d dist; run-s build",
		"restart": "run-s stop; forever start package.json; tail −−lines 0 −−follow ~/.forever/futon-media.log",
		"logs": "tail −−lines 1000 −−follow ~/.forever/futon-media.log",
		"del": "del dist node_modules",
		"stop": "forever stop futon-media || true",
		"start": "del dist; run-p --silent watch:*",
		"watch:tsc": "tsc --watch --incremental",
		"watch:node": "just-wait --silent --pattern dist; sleep 1; nodemon --quiet",
		"test": "del dist; run-s assert:build; node dist/index.js || true",
		"emby:del": "echo data/library.db* data/collections/* data/playlists/* metadata/library/* root/default/*/poster.png",
		"scripts:users-sync": "run-s stop assert:build; node dist/index.js --script users-sync"
	},
	"dependencies": {
		"all-keys": "^3.1.0",
		"ansi-colors": "^3.2.4",
		"ansi-escapes": "^4.2.0",
		"badwords": "^1.0.0",
		"cheerio": "0.22.0",
		"cli-table3": "^0.5.1",
		"dayjs": "^1.8.13",
		"deep-diff": "^1.0.2",
		"deepmerge": "^3.2.1",
		"delay": "^4.3.0",
		"dotenv": "^8.0.0",
		"eventemitter3": "^3.1.2",
		"execa": "^1.0.0",
		"exit-hook": "^2.2.0",
		"fast-json-parse": "^1.0.3",
		"fast-safe-stringify": "^2.0.6",
		"fast-xml-parser": "^3.12.16",
		"fastify": "^2.5.0",
		"ffprobe-static-next": "^4.1.3",
		"fs-extra": "^8.0.1",
		"http-errors": "^1.7.2",
		"httpie": "^1.1.2",
		"is-ip": "^3.0.0",
