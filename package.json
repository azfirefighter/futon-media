{
	"name": "futon-media-cli",
	"version": "0.3.3",
	"description": "Interactive CLI tool used to create .strm files for Emby Media",
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"bin": {
		"fm": "dist/index.js"
	},
	"scripts": {
		"reset": "del dist node_modules",
		"prepublishOnly": "del dist && tsc",
		"start": "del dist && redrun -q -p start:*",
		"start:tsc": "tsc --watch",
		"start:node": "just-wait -s -p dist && nodemon -q",
		"test": "del dist && tsc || true && node dist/index.js"
	},
	"nodemonConfig": {
		"exec": "NODE_ENV=development node --inspect dist/index.js",
		"ext": "js",
		"restartable": false,
		"watch": "dist"
	},
	"_moduleAliases": {
		"@": "dist"
	},
	"dependencies": {
		"@sindresorhus/slugify": "^0.9.1",
		"all-keys": "^3.1.0",
		"ansi-colors": "^3.2.4",
		"ansi-escapes": "^4.1.0",
		"cheerio": "^0.22.0",
		"cli-table3": "^0.5.1",
		"configstore": "^4.0.0",
		"dayjs": "^1.8.12",
		"deep-diff": "^1.0.2",
		"deepmerge": "^3.2.0",
		"delay": "^4.2.0",
		"dotenv": "^7.0.0",
		"eventemitter3": "^3.1.0",
		"execa": "^1.0.0",
		"exit-hook": "^2.2.0",
		"fast-json-parse": "^1.0.3",
		"fast-safe-stringify": "^2.0.6",
		"fastify": "^2.3.0",
		"ffprobe-static": "^3.0.0",
		"fs-extra": "^7.0.1",
		"fs-jetpack": "^2.2.2",
		"http-errors": "^1.7.2",
		"httpie": "^1.1.1",
		"ioredis": "^4.9.0",
		"js-levenshtein": "^1.1.6",
		"level": "^5.0.1",
		"leveldown": "^5.0.3",
		"levelup": "^4.0.1",
		"lodash": "^4.17.11",
		"magnet-uri": "^5.2.4",
		"mem": "^4.3.0",
		"module-alias": "^2.2.0",
		"node-env-dev": "^1.1.8",
		"node-schedule": "^1.3.2",
		"normalize-url": "^4.3.0",
		"numbro": "^2.1.2",
		"p-all": "^2.1.0",
		"p-debounce": "^2.1.0",
		"p-forever": "^2.1.0",
		"p-queue": "^5.0.0",
		"pretty-bytes": "^5.1.0",
		"pretty-ms": "^4.0.0",
		"prompts": "^2.0.4",
		"query-string": "^6.4.2",
		"read-pkg-up": "^5.0.0",
		"rxjs": "^6.4.0",
		"shimmer": "^1.2.1",
		"signal-exit": "^3.0.2",
		"sockette": "^2.0.5",
		"stacktracey": "^1.2.110",
		"string-fn": "^2.7.0",
		"string-similarity": "^3.0.0",
		"strip-ansi": "^5.2.0",
		"strip-bom": "^3.0.0",
