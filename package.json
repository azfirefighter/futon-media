{
	"name": "futon-media-cli",
	"version": "0.0.0",
	"description": "Interactive CLI tool used to create .strm files for Emby Media",
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"bin": {
		"fm": "dist/index.js"
	},
	"scripts": {
		"reset": "del dist node_modules",
		"prepublishOnly": "del dist; tsc",
		"start": "del dist; make-dir dist; run-p start:*",
		"start:node": "sleep 3; nodemon -q dist/index.js",
		"start:tsc": "tsc -w",
		"test": "del dist; tsc; node dist/index.js"
	},
	"dependencies": {
		"@sindresorhus/slugify": "^0.9.0",
		"ansi-colors": "^3.2.4",
		"ansi-escapes": "^4.1.0",
		"cheerio": "^0.22.0",
		"dayjs": "^1.8.12",
		"dotenv": "^7.0.0",
		"fast-json-parse": "^1.0.3",
		"fast-safe-stringify": "^2.0.6",
		"got": "^9.6.0",
		"js-levenshtein": "^1.1.6",
		"lodash": "^4.17.11",
		"magnet-uri": "^5.2.4",
		"mem": "^4.3.0",
		"normalize-url": "^4.3.0",
		"p-all": "^2.0.0",
		"p-debounce": "^2.0.0",
		"p-queue": "^4.0.0",
		"pretty-bytes": "^5.1.0",
		"prompts": "^2.0.4",
		"puppeteer": "^1.14.0",
		"query-string": "^6.4.2",
		"rambdax": "^2.0.0",
		"rxjs": "^6.4.0",
		"simple-get": "^3.0.3",
		"string-fn": "^2.2.0",
		"string-similarity": "^3.0.0",
		"strip-ansi": "^5.2.0"
	},
	"devDependencies": {
		"@types/ansi-colors": "^3.2.0",
		"@types/ansi-escapes": "^3.0.1",
		"@types/cheerio": "^0.22.11",
		"@types/dotenv": "^6.1.1",
		"@types/got": "^9.4.1",
		"@types/lodash": "^4.14.123",
		"@types/magnet-uri": "^5.1.2",
		"@types/node": "^11.13.0",
		"@types/p-all": "^1.0.0",
		"@types/p-queue": "^3.2.1",
		"@types/pretty-bytes": "^5.1.0",
		"@types/prompts": "^1.2.0",
		"@types/puppeteer": "^1.12.3",
		"@types/query-string": "^6.3.0",
		"@types/shimmer": "^1.0.1",
		"@types/string-similarity": "^3.0.0",
		"del-cli": "^1.1.0",
		"dts-generate": "^1.0.1",
		"make-dir-cli": "^2.0.0",
		"node-env-dev": "^1.1.1",
		"nodemon": "^1.18.10",
		"npm-run-all": "^4.1.5",
		"shimmer": "^1.2.1",
		"typescript": "^3.4.1"
	},
