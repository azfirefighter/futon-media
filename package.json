{
	"name": "futon-media",
	"version": "1.0.0",
	"description": "Instantly stream movies and TV shows on Emby Media",
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"_moduleAliases": {
		"@": "dist"
	},
	"scripts": {
		"deploy": "run-s repo build restart",
		"repo": "git pull; echo; git reset --hard origin/master",
		"build": "tsc || true",
		"assert:build": "test ! -d dist; run-s build",
		"restart": "run-s stop; forever start forever.json; tail -f -n 0 ~/.forever/futon-media.log",
		"logs": "tail -f -n 1000 ~/.forever/futon-media.log",
		"del": "del dist node_modules",
		"stop": "forever stop futon-media || true",
		"start": "del dist; run-p --silent watch:*",
		"watch:tsc": "tsc --watch --incremental",
		"watch:node": "just-wait --silent --pattern dist/index.js && watchexec --restart --debounce 100 --watch dist --exts js -- NODE_ENV=development node --inspect dist/index.js",
		"test": "del dist; run-s assert:build; node dist/index.js || true",
		"emby:restart": "echo 'killall EmbyServer; sleep 1; open -a EmbyServer; tail -f -n 100 ~/.config/emby-server/logs/embyserver.txt | sedbat'",
		"emby:del": "echo 'rm -rf ~/.config/emby-server/cache/*; rm -rf ~/.config/emby-server/data/activitylog.*; rm -rf ~/.config/emby-server/data/collections/*; rm -rf ~/.config/emby-server/data/library.*; rm -rf ~/.config/emby-server/data/playlists/*; rm -rf ~/.config/emby-server/logs/*; rm -rf ~/.config/emby-server/media/movies/*; rm -rf ~/.config/emby-server/media/shows/*; rm -rf ~/.config/emby-server/metadata/library/*; rm -rf ~/.config/emby-server/root/*; rm -rf ~/.config/emby-server/transcoding-temp;'",
		"scripts:users-sync": "run-s build stop; node dist/index.js --scripts users-sync"
	},
