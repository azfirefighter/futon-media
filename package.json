{
	"name": "futon-media-cli",
	"version": "0.0.0",
	"description": "Interactive CLI tool used to create .strm files for Emby Media",
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"bin": {
		"fm": "dist/index.js"
	},
	"scripts": {
		"reset": "del dist node_modules",
		"prepublishOnly": "del dist && tsc",
		"start": "del dist && redrun -q -p start:*",
		"start:tsc": "tsc --watch",
		"start:node": "just-wait -s -p dist && nodemon -q",
		"test": "del dist && tsc; node dist/index.js"
	},
	"nodemonConfig": {
		"ext": "js",
		"restartable": false,
		"watch": "dist"
	},
	"dependencies": {
		"@sindresorhus/slugify": "^0.9.1",
		"all-keys": "^3.1.0",
		"ansi-colors": "^3.2.4",
		"ansi-escapes": "^4.1.0",
		"cheerio": "^0.22.0",
		"dayjs": "^1.8.12",
		"dotenv": "^7.0.0",
		"fast-json-parse": "^1.0.3",
		"fast-safe-stringify": "^2.0.6",
		"http-errors": "^1.7.2",
		"js-levenshtein": "^1.1.6",
		"lodash": "^4.17.11",
		"magnet-uri": "^5.2.4",
		"mem": "^4.3.0",
		"normalize-url": "^4.3.0",
		"p-all": "^2.1.0",
		"p-debounce": "^2.1.0",
		"p-queue": "^5.0.0",
		"pretty-bytes": "^5.1.0",
		"prompts": "^2.0.4",
		"puppeteer": "^1.14.0",
		"query-string": "^6.4.2",
		"rxjs": "^6.4.0",
		"simple-get": "^3.0.3",
		"string-fn": "^2.2.0",
		"string-similarity": "^3.0.0",
		"strip-ansi": "^5.2.0",
		"ts-optchain": "^0.1.5",
		"url-parse": "^1.4.4"
	},
	"devDependencies": {
		"@types/ansi-colors": "^3.2.0",
		"@types/ansi-escapes": "^3.0.1",
		"@types/cheerio": "^0.22.11",
		"@types/dotenv": "^6.1.1",
		"@types/http-errors": "^1.6.1",
		"@types/lodash": "^4.14.123",
		"@types/magnet-uri": "^5.1.2",
		"@types/node": "^11.13.2",
		"@types/p-all": "^1.0.0",
		"@types/p-queue": "^3.2.1",
		"@types/pretty-bytes": "^5.1.0",
		"@types/pretty-format": "^20.0.1",
		"@types/prompts": "^1.2.0",
		"@types/puppeteer": "^1.12.3",
		"@types/qs": "^6.5.2",
		"@types/query-string": "^6.3.0",
		"@types/shimmer": "^1.0.1",
		"@types/string-similarity": "^3.0.0",
		"@types/url-parse": "^1.4.3",
		"del-cli": "^1.1.0",
		"dts-generate": "^1.0.1",
		"just-wait": "^1.0.11",
		"make-dir-cli": "^2.0.0",
		"node-env-dev": "^1.1.2",
		"nodemon": "^1.18.11",
		"redrun": "^7.1.5",
		"shimmer": "^1.2.1",
		"typescript": "^3.4.2"
	},
